{
    "@npm/semantic-ui": {
        "*": {
            "files":  [ "dist/semantic.*", "dist/themes/**" ],
            "=dependencies": {
                "jquery": "x.*",
            },
            "=scripts": {},
            "pak": {
                "export": [{
                    from: "dist/themes/default/assets/**",
                    "trim": 1
                }, {
                    from: "dist/semantic.*",
                    "trim": 1
                }],
                "render": {
                    "js": [ "*.js" ],
                    "css": [ "*.css" ],
                }
            }
        }
    },
    "Semantic-Org/Semantic-UI-CSS": {
        "*": {
            "name": "semantic-ui",
            "files":  [ "semantic.*", "themes/**" ],
            "pak": {
                "export": [{
                    from: "themes/default/assets/**"
                }, {
                    from: "semantic.*"
                }],
                "render": {
                    "js": [ "*.js" ],
                    "css": [ "*.css" ],
                }
            }
        }
    }
}
